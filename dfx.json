{
  "canisters": {
    "ICPad_backend": {
      "dependencies": [
        "ICPad_user"
      ],
      "candid": "src/ICPad_backend/ICPad_backend.did",
      "package": "ICPad_backend",
      "type": "rust"
    },
    "ICPad_frontend": {
      "dependencies": [
        "ICPad_backend",
        "ICPad_marketplace"
      ],
      "source": [
        "src/ICPad_frontend/dist"
      ],
      "type": "assets",
      "workspace": "ICPad_frontend"
    },
    "ICPad_user":{
      "package": "ICPad_user",
      "candid": "src/ICPad_user/ICPad_user.did",
      "type": "rust"
    },
    "ICPad_marketplace": {
        "package": "ICPad_marketplace",
        "candid": "src/ICPad_marketplace/ICPad_marketplace.did",
        "type": "rust"
    },
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
